##
###	#PCoA with Bray-Curtis Distances#	###
ord <- ordinate(VOC, method = "PCoA", distance = "bray")
plot_ordination(VOC, ord, color = "day", shape = "microbe.treatment", type = "samples")
plot_ordination(VOC, ord, type = "split", color = "day", shape = "microbe.treatment", label = "Name")
PCs <- data.frame(scores(ord$vectors))
new.dat <- cbind(PCs, sample_data(VOC)$microbe.treatment, sample_data(VOC)$day, sample_data(VOC)$nectar.conc) # make a new matrix
names(new.dat)[40:42] <- c("Microbe", "Day", "Nectar")
library(gdata)
new.dat$Microbe <- reorder.factor(new.dat$Microbe, new.order = c("MrAa", "Aa", "Mr"))
new.dat$Nectar <- reorder.factor(new.dat$Nectar, new.order = c("normal", "low"))
ggplot(new.dat, aes(x=Axis.1, y= Axis.2)) +
geom_point(aes(color = Microbe, shape = Nectar), position ="jitter") +
facet_grid(~Day)+
theme_bw()+
xlab("PCoA1 (33.5% Var. Expl.)")+ 	## !don't forget to update these moving forward!
ylab("PCoA2 (25.4% Var. Expl.)")
